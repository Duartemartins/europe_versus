                <div class="flex items-center">
                  <span class="text-sm font-medium text-slate-700 mr-2"><%= country[:flag] %></span>
                  <span class="text-sm font-medium text-slate-700"><%= country[:name] %></span>
                </div>
                <div class="text-right">
                  <% if gdp_data && gdp_data[:value] %>
                    <div class="<%= country[:value_class] %>">
                      $<%= number_with_delimiter(gdp_data[:value].to_i) %>
                    </div>
                    <div class="text-xs text-slate-500"><%= gdp_data[:year] %></div>
                  <% else %>
                    <div class="text-slate-400 font-medium">No data</div>
