<!-- Modern Hero Section with animated gradient -->
<section class="relative text-center mb-20">
  <div class="absolute inset-0 bg-gradient-to-r from-blue-600/10 via-indigo-600/10 to-purple-600/10 rounded-3xl transform -skew-y-1"></div>
  <div class="relative max-w-5xl mx-auto py-20 px-8">
    <div class="animate-fade-in">
      <h1 class="text-5xl md:text-7xl font-black mb-8">
        <span class="bg-gradient-to-r from-slate-900 via-blue-900 to-indigo-900 bg-clip-text text-transparent">Europe</span>
        <span class="bg-gradient-to-r from-blue-600 to-indigo-600 bg-clip-text text-transparent block md:inline"> Versus</span>
      </h1>
      <p class="text-2xl md:text-3xl text-slate-600 mb-8 font-light">
        Evidence-based comparisons with the US, India, and China
      </p>
      <p class="text-xl text-slate-500 mb-12 max-w-3xl mx-auto leading-relaxed">
        Counter narratives about Europe with factual data. 
        Discover how European countries perform across key metrics compared to major global economies.
      </p>
      <%= link_to "Explore Statistics", statistics_path, 
          class: "inline-flex items-center bg-gradient-to-r from-blue-600 to-indigo-600 text-white px-10 py-4 rounded-2xl text-xl font-semibold hover:from-blue-700 hover:to-indigo-700 transition-all duration-300 shadow-xl hover:shadow-2xl transform hover:-translate-y-1" %>
    </div>
  </div>
  
  <!-- Floating elements -->
  <div class="absolute top-20 left-10 w-20 h-20 bg-blue-200 rounded-full opacity-20 animate-bounce" style="animation-delay: 0s;"></div>
  <div class="absolute top-40 right-20 w-16 h-16 bg-indigo-200 rounded-full opacity-20 animate-bounce" style="animation-delay: 2s;"></div>
  <div class="absolute bottom-20 left-1/4 w-12 h-12 bg-purple-200 rounded-full opacity-20 animate-bounce" style="animation-delay: 4s;"></div>
</section>

<!-- Modern Stats Preview with glass morphism -->
