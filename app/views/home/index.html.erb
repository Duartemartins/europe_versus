                  <% else %>
                    <div class="text-slate-400 font-medium">No data</div>
                  <% end %>
                </div>
              </div>
            <% end %>
          </div>
          <div class="mt-6">
            <%= link_to "/statistics/population", class: "text-purple-600 hover:text-purple-800 text-sm font-medium transition-colors duration-200 hover:underline" do %>
              View detailed population data ‚Üí
            <% end %>
          </div>
        </div>
      </div>
    </div>

